
<h1> <%= @dojo.branch %> Dojo </h1>
<br>
Address:<%= @dojo.street %>
<br>
City: <%= @dojo.city %> State: <%= @dojo.satate %>
<br><br>
<a href="/dojos"> Home </a> | <a href="/dojos/<%= @dojo.id %>/edit"> Edit </a>
<br><br>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name </th>
        <th>Email</th>
        <th> Actions </th>
    </tr>
    <% @students.each do |student| %>
    <tr>
        <td> <%= student.first_name %> </td>
        <td> <%= student.last_name %> </td>
        <td> <%= student.email %> </td>
        <td> <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>"> Show</a> | <a href='/dojos/<%= @dojo.id %>/students/<%= student.id %>/edit'> Edit </a>| <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>" data-method="delete"> Destroy</a>
    </tr> 
    <% end %>
</table>
<br><br>
<a href="/dojos/<%= @dojo.id %>/students/new"> Add Student</a>

